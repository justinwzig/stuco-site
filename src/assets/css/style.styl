.top1
    margin-top 2%

.resp-tabs-list
    margin 2.5% 0 3.3%
    padding 0
    list-style none
    text-align center
    border 1px solid #969696
    border-right 0
    border-left 0

.resp-tab-item
    display inline-block
    padding 16px 0px
    span
        padding 16px 30px
        text-decoration none
        color #FC635E
        cursor pointer
        font-size 1em
        font-weight 400
        text-transform uppercase
        border-radius 4px
        margin-right 0.5em

.resp-tabs-container
    padding 0px
    background-color #fff
    clear left

h2
    &.resp-accordion
        cursor pointer
        padding 5px
        display none

.resp-tab-content
    display none

.resp-content-active, .resp-accordion-active
    display block

.tab_img
    padding 1em 0em
    display block

.img-top
    position relative
    &:hover
        .link-top
            display none

.link-top
    position absolute
    top 0%
    text-align center
    width 90%
    background rgba(0, 0, 0, 0.75)
    height 100%

.port-img
    a
        overflow hidden
        position relative
        display block
        .captn
            display inline-block
            font-weight 800
            text-transform uppercase
            color #ffffff
            top 0%
            position absolute
            left -50%
            right 0
            background-color rgba(214, 95, 88, 0.95)
            padding 29% 0
            position absolute
            height 100%
            width 50%
            top 0
            left -50%
            text-align center
            -o-transition all 0.3s ease
            -moz-transition all 0.3s ease
            -ms-transition all 0.3s ease
            -webkit-transition all 0.3s ease
            transition all 0.3s ease
        &:hover
            .captn
                width 100%
                left 0%